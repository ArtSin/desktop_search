{
  "build": {
    "beforeDevCommand": "cd client_ui && trunk serve",
    "beforeBuildCommand": "cd client_ui && trunk build --release",
    "devPath": "http://localhost:1420",
    "distDir": "../client_ui/dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "client",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": false,
      "identifier": "org.artsin.desktop-search",
      "category": "Productivity",
      "shortDescription": "",
      "longDescription": "",
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "targets": "all",
      "deb": {
        "depends": []
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "width": 1280,
        "height": 720,
        "resizable": true,
        "title": "Локальный поиск"
      }
    ]
  }
}